@using System.Data.SqlClient;
@using System.Configuration;
@model IEnumerable<CarMarket.Domain.Entity_Models_.Car>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>title</h2>
@foreach (var car in Model)
{
    <p>@car.Name</p>
    <p>@car.Model</p>
    <p>@car.Description</p>
    <p>@car.Price</p>
    <p>@car.Speed</p>
    <p>@car.DateCreate</p>
}
